{
  "name": "react-perf-tool",
  "version": "0.2.2",
  "description": "Record and debug React performance from the browser",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "babel --ignore spec.js,story.js,*.spec.js ./src --out-dir ./dist",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "jest --coverage",
    "lint": "eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RamonGebben/react-perf-tool.git"
  },
  "keywords": [
    "react-component",
    "react",
    "performance"
  ],
  "author": "Ramon Gebben <ramon@ra-ge.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RamonGebben/react-perf-tool/issues"
  },
  "homepage": "https://github.com/RamonGebben/react-perf-tool",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^23.1.0",
    "react": "^16.4.1",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^16.4.1",
    "regenerator-runtime": "^0.11.1"
  },
  "dependencies": {
    "chart.js": "^2.7.2",
    "keymaster": "^1.6.2",
    "styled-components": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-addons-perf": "^0.14.0 || ^15.4.2"
  }
}
